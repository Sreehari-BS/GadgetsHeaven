            <br><br><br><br>
            <% if (!wishlists || wishlists.wishList.filter(function(item) { return item.is_in_wishlist===true;
                }).length===0) { %>
                <h4 class="text-center text-danger">No Wishlists are Available.</h4>
                <% } else { %>
                    <table class="container" style="width: 100%;">
                        <tr>
                            <th>No:</th>
                            <th>Image</th>
                            <th>Name</th>
                            <th>Price</th>
                            <th>Action</th>
                        </tr>
                        <% wishlists.wishList.filter(function(item) { return item.is_in_wishlist===true;
                            }).forEach(function(wishListItem, index) { %>
                            <tr>
                                <td>
                                    <%= index + 1 %>
                                </td>
                                <td>
                                    <img src="<%= wishListItem.product.images[2] %>" alt="img"
                                        width="100">
                                </td>
                                <td>
                                    <%= wishListItem.productName %>
                                </td>
                                <td>
                                    <%= wishListItem.product.price %>
                                </td>
                                <td>
                                    <form
                                        action="/home/addToCartFromWishlist/<%= wishlists._id %>/<%= wishListItem.product._id %>"
                                        method="post">
                                        <button type="submit" class="btn btn-outline-success">Add to Cart</button>
                                    </form>
                                </td>
                                <td>
                                    <a
                                        href="/home/removeProductFromWishlist/<%= wishListItem.product._id  %>/<%= wishlists._id %>">
                                        <button class="btn btn-outline-danger">Remove</button>
                                    </a>
                                </td>
                            </tr>
                            <% }); %>
                    </table>
                    <% } %>




